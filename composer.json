{
    "name": "teskon/psd2php",
    "description": "PSD2 package for easy use of the public Banking APIs.",
    "keywords": ["psd2", "composer", "package"],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Kristian Auestad",
            "email": "kristianaue@gmail.com",
            "role": "Developer"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "~6.3"
    },
    "require-dev": {
		"phpunit/phpunit": "6.*"
    },
    "suggest": {
        "ext-curl": "Allows for quicker response times when connecting to the banking APIs."
    },
    "autoload": {
        "psr-4": {
            "Teskon\\PSD2PHP\\": "src/"
        }
    }
}
